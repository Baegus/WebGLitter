{
	"name": "webglitter",
	"author": "Jaroslav PetrnouÅ¡ek",
	"version": "1.0.0",
	"description": "A Parcel template for my portfolio website",
	"browserslist": "> 0.25%, last 2 versions, not dead",
	"targets": {
		"public": {
			"distDir": "dist",
			"publicUrl": "./",
			"source": [
				"src/pug/index.pug"
			]
		}
	},
	"scripts": {
		"dev": "node ./parcel/runDev.mjs",
		"debug": "node ./parcel/runDev.mjs --debug",
		"prebuild": "node ./parcel/runPrebuild.mjs",
		"build": "parcel build --no-source-maps --log-level error --config ./parcel/.parcel.prod"
	},
	"devDependencies": {
		"@parcel/transformer-pug": "^2.15.2",
		"@parcel/transformer-sass": "^2.15.2",
		"express": "^4.19.2",
		"parcel": "^2.15.2",
		"parcel-reporter-clean-dist": "^1.0.4",
		"parcel-reporter-static-files-copy": "^1.5.3"
	},
	"staticFiles": {
		"staticPath": "static"
	},
	"dependencies": {
		"animejs": "^4.0.2"
	}
}
